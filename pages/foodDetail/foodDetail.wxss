page {
  background-color: #fff;
}
.foodMake_container .navigationbar {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  z-index: 10;
  background-color: #fffaf6;
}

.foodMake_container .navigationbar .status {
  width: 100%;
}

.foodMake_container .navigationbar .navbar {
  display: flex;
  align-items: center;
}

.foodMake_container .navigationbar .back_icon {
  display: flex;
  align-items: center;
  margin-left: 24rpx;
}

.foodMake_container .navigationbar .icon_img {
  width: 22rpx;
  height: 36rpx;
}

.foodMake_container .navigationbar .icon_home {
  width: 36rpx;
  height: 36rpx;
  margin-left: 36rpx;
}

.foodMake_container .navigationbar .nav_title {
  font-size: 32rpx;
  color: #000;
  text-align: center;
  margin-left: 35%;
  font-weight: bold;
  transform: translateX(-50%);
}

@keyframes move {
  0% {
    top: 0rpx;
  }

  50% {
    top: -10rpx;
  }

  100% {
    top: -20rpx;
  }
}

.foodMake_container .bgImg {
  position: absolute;
  left: 0;
  right: 0;
  width: 100%;
  height: 42vh;
  /* height: 589rpx; */
  z-index: 3;
}

.foodMake_container .bottle {
  position: absolute;
  left: 0;
  top: -180rpx;
  width: 130rpx;
  height: 130rpx;
  z-index: 100;
  overflow: visible;
}

.foodMake_container .bottle image {
  width: 130rpx;
  height: 130rpx;
}

.foodMake_container .bottle .bottle-transiton {
  position: absolute;
  left: 33rpx;
  top: -18rpx;
  width: 20rpx;
  height: 30rpx;
  visibility: hidden;
}

.foodMake_container .bottle .bottle-transiton.vis_v {
  visibility: visible;
}

.foodMake_container .foodDetail_con_top {
  position: fixed;
  width: 100%;
  z-index: 4;
  top: 8vh;
  display: flex;
  height: 42vh;
  align-items: center;
  justify-content: center;
}

.foodMake_container .foodDetail_con_top .food_img {
  width: 64vw;
  height: 64vw;
}

.foodMake_container .big_box {
  height: 50vh;
  width: 100vw;
  position: fixed;
  bottom: 0;
  left: 0;
}

.foodMake_container .powder_wrap {
  position: absolute;
  width: 100rpx;
  height: 200rpx;
  left: 46%;
  top: 20%;
  margin-left: -50rpx;
  opacity: 0;
  z-index: 6;
}

.foodMake_container .foodDetail_con_center {
  width: 690rpx;
  height: 28%;
  margin: 0 auto;
  background: rgba(254, 203, 164, 0.1);
  color: #666;
  font-size: 24rpx;
  text-align: center;
}

.foodDetail_con_center_list {
  height: 100%;
  display: flex;
  flex-wrap: wrap;
  border: 2rpx dashed #f6e7af;
  box-sizing: border-box;
}

.foodDetail_con_center .seasoning-item {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 30vw;
  border-right: 2rpx dashed rgba(254, 143, 113, 0.3);
  border-bottom: 2rpx dashed rgba(254, 143, 113, 0.3);
}

.foodDetail_con_center .seasoning-item:nth-child(3n) {
  border-right: none;
}

.foodDetail_con_center .seasoning-item.active {
  color: #fecba4;
}

.foodDetail_con_center_btm {
  display: flex;
  align-items: center;
  justify-content: center;
  overflow: hidden;
  height: 10%;
  background: rgba(254, 143, 113, 0.14);
  position: relative;
}

.tips_text {
  font-size: 22rpx;
  text-align: center;
  color: #fe8f71;
}

.foodDetail_con_center_btm_left {
  position: absolute;
  left: -34rpx;
  width: 43rpx;
  height: 133rpx;
  z-index: 1;
  background: #fff;
  transform: rotate(21deg);
  top: -20rpx;
}

.foodDetail_con_center_btm_right {
  position: absolute;
  right: -27rpx;
  width: 43rpx;
  height: 100rpx;
  z-index: 1;
  background: #fff;
  transform: rotate(-21deg);
  top: -19rpx;
}

.foodDetail_con_footer {
  height: 38%;
  padding: 16rpx 0;
  background: #fecba4;
}

.list_wrap {
  overflow-x: auto;
  overflow-y: hidden;
  display: flex;
  height: 100%;
  flex-wrap: nowrap;
  padding-right: 24rpx;
  background: #ffddc2;
}

.foodDetail_con_footer .foodDetail_con_footer_item {
  min-width: 144rpx;
  height: 84%;
  display: flex;
  justify-content: center;
  box-shadow: 0px 4px 10px 0px rgba(4, 0, 0, 0.1);
  border: 2rpx solid #fecba4;
  color: #353535;
  font-size: 22rpx;
  overflow: visible;
  margin: 3% 0;
  margin-left: 24rpx;
  position: relative;
  border-radius: 12rpx;
}

.foodDetail_con_footer_item_topInner {
  position: absolute;
  top: 0rpx;
  margin: 0 auto;
  color: #fff;
  font-size: 30rpx;
  visibility: hidden;
}

.foodDetail_con_footer_item_topInner.active {
  visibility: visible;
  animation: move 2000 easy 500 1 backwards;
  -moz-animation: move 2000 easy 500 1 backwards;
  -webkit-animation: move 2000 easy 500 1 backwards;
  -o-animation: move 2000 easy 500 1 backwards;
  display: block;
}

.foodDetail_con_footer_item image {
  width: 130rpx;
  height: 130rpx;
}

.foodDetail_con_footer_item .foodDetail_con_footer_item_btn {
  position: absolute;
  bottom: 0;
  cursor: pointer;
  height: 48rpx;
  width: 142rpx;
  border-radius: 34rpx 34rpx 12rpx 12rpx;
  border: 3rpx solid #fff;
  line-height: 48rpx;
  overflow: hidden;
  text-align: center;
  background: #fecba4;
}
.content_wrap {
  height: 76%;
}
.position_action {
  position: fixed;
  bottom: 0;
  background: white;
  width: 100%;
  height: 19.5%;
}

.action {
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 32rpx;
  padding: 16rpx 30rpx;
  height: 14%;
}

.action > .confirm {
  flex: 2;
  height: 88rpx;
  padding: 16rpx 30rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #fff;
  background: linear-gradient(
    -90deg,
    rgba(254, 143, 113, 1),
    rgba(254, 203, 164, 1)
  );
  border-radius: 44rpx;
}

.action > .reset {
  height: 88rpx;
  flex: 1;
  border: none;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #b2b2b2;
}

.iphonexWrap .big_box {
  height: 42vh;
  bottom: 68rpx;
}

.iphonexWrap .foodDetail_con_footer_item image {
  margin-top: 20rpx;
}

.iphonexWrap .foodDetail_con_top {
  top: 9vh;
}
